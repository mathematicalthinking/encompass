{
  "name": "encompass",
  "version": "2.3.0",
  "private": true,
  "description": "enCoMPASS",
  "keywords": [
    "assessment",
    "rubrics",
    "education"
  ],
  "homepage": "https://github.com/mathematicalthinking/encompass#readme",
  "bugs": {
    "url": "https://github.com/mathematicalthinking/encompass/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathematicalthinking/encompass.git"
  },
  "license": "AGPL-3.0",
  "author": "Mathematical Thinking",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start-front": "ember server --proxy=http://localhost:8080",
    "start-back": "node app_server/server.js",
    "test-back": "NODE_ENV=seed node app_server/server.js",
    "start-all": "concurrently \"npm:start-front\" \"npm:start-back\"",
    "test": "npm-run-all lint test:*",
    "start-test": "ember server --proxy=http://localhost:8082",
    "selenium": "./node_modules/.bin/mocha test/selenium/*.js",
    "test:ember": "ember test",
    "seed": "node test/seedDb",
    "drop-testdb": "echo 'db.dropDatabase()' | mongo encompass_test",
    "seed-vmt": "cd ../vmt/server && npm run test",
    "seed-mt": "cd ../mt-sso && npm run test",
    "seed-all": "concurrently \"grunt serve-seed\" \"npm:seed-vmt\" \"npm:seed-mt\"",
    "restore-mt": "cd ../mt-sso && npm run seed",
    "restore-vmt": "cd ../vmt/server && npm run seed",
    "dev-vmt": "cd ../vmt/server && npm run dev",
    "dev-mt": "cd ../mt-sso && npm run dev",
    "dev-all": "concurrently \"npm:start-front\" \"npm:start-back\" \"npm:dev-mt\" \"npm:dev-vmt\"",
    "test-travis": "NODE_ENV=seed node server/server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-paginate": "^1.0.2",
    "express-session": "^1.17.1",
    "g": "^2.0.1",
    "gm": "^1.23.1",
    "gm-reloaded": "^1.24.0",
    "htmlparser2": "^6.0.1",
    "http": "0.0.0",
    "http-errors": "^1.8.0",
    "i": "^0.3.6",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nconf": "^0.11.2",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.7",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pdf2pic": "^2.1.4",
    "q": "^1.5.1",
    "randomcolor": "^0.6.2",
    "request": "^2.88.2",
    "sharp": "^0.28.1",
    "socket.io": "^2.4.0",
    "underscore": "^1.13.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@ember/jquery": "^1.1.0",
    "@ember/optional-features": "^1.3.0",
    "@ember/test-helpers": "^2.2.5",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chromedriver": "^91.0.1",
    "concurrently": "^6.0.1",
    "csvtojson": "^2.0.10",
    "ember-auto-import": "^1.11.2",
    "ember-cli": "~3.25.3",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-cookies": "^0.5.2",
    "ember-data": "~3.26.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-lodash": "^4.19.5",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.26.1",
    "ember-template-lint": "^3.2.0",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "geckodriver": "^2.0.1",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "mocha": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0",
    "sass": "^1.32.8",
    "selenium-webdriver": "^4.0.0-beta.4"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  }
}
