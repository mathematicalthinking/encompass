<ResponseContainer
  @store={{this.store}}
  @response={{this.model.response}}
  @submission={{this.model.submission}}
  @workspace={{this.model.workspace}}
  @submissions={{this.model.submissions}}
  @notifications={{this.model.notifications}}
  @toResponseInfo='toResponseInfo'
  @toResponse='toResponse'
  @responses={{this.model.responses}}
  @toResponses='toResponses'
  @toResponseSubmission='toResponseSubmission'
  @toNewResponse='toNewResponse'
  @storeResponses={{this.model.allResponses}}
/>